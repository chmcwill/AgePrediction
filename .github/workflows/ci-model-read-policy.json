{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "ListModelPrefix",
      "Effect": "Allow",
      "Action": ["s3:ListBucket"],
      "Resource": "arn:aws:s3:::agepred-model-artifacts",
      "Condition": {
        "StringLike": {
          "s3:prefix": ["models/v2026-02-25/*", "models/v2026-02-25"]
        }
      }
    },
    {
      "Sid": "ReadModelObjects",
      "Effect": "Allow",
      "Action": ["s3:GetObject"],
      "Resource": "arn:aws:s3:::agepred-model-artifacts/models/v2026-02-25/*"
    }
  ]
}
